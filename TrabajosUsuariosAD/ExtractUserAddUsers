Get-MsolUser | Select-Object UserPrincipalName, DisplayName, Department | Export-Csv -Path "C:\ruta\usuarios.csv" -NoTypeInformation
# Importar el módulo de AzureAD
Import-Module AzureAD

# Conectarse al nuevo tenant
Connect-AzureAD

# Leer el archivo CSV
$usuarios = Import-Csv -Path "C:\ruta\usuarios.csv"

# Agregar cada usuario al nuevo tenant
foreach ($usuario in $usuarios) {
    New-AzureADUser -UserPrincipalName $usuario.UserPrincipalName -DisplayName $usuario.DisplayName -Department $usuario.Department -PasswordProfile (New-Object -TypeName Microsoft.Open.AzureAD.Model.PasswordProfile -Property @{Password = "ContraseñaTemporal123"; ForceChangePasswordNextSignIn = $true})
}
